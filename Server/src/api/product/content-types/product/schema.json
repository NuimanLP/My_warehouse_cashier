{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Product_name": {
      "type": "string"
    },
    "Barcode": {
      "type": "string"
    },
    "Pricing": {
      "type": "decimal"
    },
    "StockQuantity": {
      "type": "integer"
    },
    "Description": {
      "type": "text"
    },
    "transactions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::transaction.transaction",
      "mappedBy": "products"
    },
    "Shot": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Branding": {
      "type": "string"
    },
    "Category": {
      "type": "string"
    }
